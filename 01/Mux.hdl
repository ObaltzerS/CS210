/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=sel, b=sel, out=notsel);
    Nand(a=sel, b=b, out=bsel);
    Nand(a=notsel, b=a, out=asel);
    Nand(a=asel, b=bsel, out=out);
}